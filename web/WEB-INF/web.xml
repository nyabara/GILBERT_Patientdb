<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>register</servlet-name>
        <servlet-class>User.register</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>User.Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>logout</servlet-name>
        <servlet-class>User.logout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>addpatients</servlet-name>
        <servlet-class>User.addpatients</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Load_Patients</servlet-name>
        <servlet-class>User.Load_Patients</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ActivePatients</servlet-name>
        <servlet-class>User.ActivePatients</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UpdatePatients</servlet-name>
        <servlet-class>User.UpdatePatients</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SelectPatient</servlet-name>
        <servlet-class>User.SelectPatient</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>register</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>logout</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>addpatients</servlet-name>
        <url-pattern>/addpatients</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Load_Patients</servlet-name>
        <url-pattern>/Load_Patients</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ActivePatients</servlet-name>
        <url-pattern>/ActivePatients</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UpdatePatients</servlet-name>
        <url-pattern>/UpdatePatients</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SelectPatient</servlet-name>
        <url-pattern>/SelectPatient</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
